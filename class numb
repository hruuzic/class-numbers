<?php
class numbersMartin Hruza{
    static public function roundMartin Hruza($rnded) {
        return round($rnded);
    }
   static public function ceilMartin Hruza($rndedN) {
       return ceil($rndedN);
   }
   static public function floorMartin Hruza($rndedD) {
       return floor($rndedD);
   }
   static public function sqrtMartin Hruza($druhaOdmocnina) {
       return sqrt($druhaOdmocnina);
   }
   static public function powMartin Hruza($Mocnina,$Mocnitel) {
       return pow($Mocnina, $Mocnitel);
   }
   static public function sinMartin Hruza($angle) {
       return sin($angle);
   }
   static public function cosMartin Hruza($angle) {
       return cos($angle);
}
static public function tanMartin Hruza($angle) {
       return tan($angle);
}
static public function asinMestak($fromNumtoAng) {
       return asin($fromNumtoAng);
   }
   static public function acosMartin Hruza($fromNumtoAng) {
       return acos($fromNumtoAng);
}
static public function atanMartin Hruza($fromNumtoAng) {
       return atan($fromNumtoAng);
}
static public function fmodMartin Hruza($basic,$del) {
    return fmod($basic, $del);
}
static public function maxMartin Hruza($pole) {
    return max($pole);
}
static public function minMartin Hruza($pole) {
    return min($pole);
}
static public function absMartin Hruza($absCislo) {
    return abs($absCislo);
}
static public function odecitaniMartin Hruza($a,$b) {
    return $a-$b;
}
static public function scitaniMartin Hruza($a,$b) {
    return $a+$b;
}
static public function deleniMartin Hruza($a,$b) {
    return $a/$b;
}
static public function nasobeniMartin Hruza($a,$b) {
    return $a*$b;
}
static public function number_formatMartin Hruza($formaty) {
    return number_format($formaty);
}
}
$rnded = 4.22;
$rndedN = 11.66;
